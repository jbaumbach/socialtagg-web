extends layout

block content
  .container
    .sixteen.columns
      //
         Headline 
      .headline.no-margin
        h3 SocialTagg Pricing
      a(href=links.myownedevents)
        img.screen-960(src='/images/30-Day-Trial-Banner-960.png')
      a(href=links.myownedevents)
        img.screen-768(src='/images/30-Day-Trial-Banner-768.png')
      a(href=links.myownedevents)
        img.screen-480(src='/images/30-Day-Trial-Banner-480.png')
      a(href=links.myownedevents)
        img.screen-300(src='/images/30-Day-Trial-Banner-300.png')
      //
         Tabs Navigation 
      ul.tabs-nav
        li.active
          a(href='#tab1') A La Carte
        li
          a(href='#tab2') Subscription
      //
         Tabs Content 
      .tabs-container
        #tab1.tab-content
          //
            Start Tab1 Content
          .container
            .sixteen.columns
              //
                 Number of Tables / From 2 to 6 / 
              .six-tables
                .tab.title-column
                  //
                    TP 1/5
                  .color-1
                    h3 Event Size
                    h4
                      span.price Price
                      span.time per event
                    ul
                      li
                        span.thicker Max Check-Ins
                        span.smaller-text Attendees
                      li.thicker.two-line-center-single Event Type
                      li.thicker.two-line-center-single Analytics
                      li.thicker.two-line-center-single Survey
                .tab.pricing-table
                  .color-2
                    h3 Free Event
                    h4
                      span.price.line-filler Free
                    ul
                      li.two-line-center-single 50
                      li.two-line-center-single Social Function
                      li.two-line-center-single
                        img(src='/images/checkmark.png')
                      li.two-line-center-single
